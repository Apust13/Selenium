{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "yandex.by"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "text"
      },
      "text": "Привет"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//tr[@class='search__row']//button[.='Найти']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//table[@class='search__table']/tbody/tr/td[1]/span/span[1]/span"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img.logo__image"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}